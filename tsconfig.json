{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "types": ["react-dom/experimental", "node"],
    "resolveJsonModule": true,
  },
  "include": [
    "app/javascript/**/*",
    "app/src/**/*"
, "app/src/types", "app/src/components"  ],
  "exclude": [
    "node_modules",
    "tmp",
    "public"
  ]
}